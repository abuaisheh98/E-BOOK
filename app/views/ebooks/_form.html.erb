<div class="container">
  <%= form_for(@ebook) do |f| %>
    <div class="field form-group">
      <%= f.label :'Book Name' %>
      <%= f.text_field :book_name ,class:"form-control" %>
    </div>

    <div class="field">
      <%= f.label :'Author Name' %>
      <%= f.text_field :author_name ,class:"form-control"%>
    </div>

    <div class="field">
      <%= f.label :'description' %>
      <%= f.text_field :description,class:"form-control" %>
    </div>

    <div class="field">
      <%= f.label :publish_year %>
      <%= f.text_field :publish_year ,class:"form-control"%>
    </div>

    <div class="field">
      <%= f.label :ratting %>
      <%= f.text_field :rate ,class:"form-control"%>
    </div>
    <br>
    <%= f.submit class:"btn btn-success"%>

    <%= link_to 'Back', ebooks_path,class:"btn btn-secondary" %>

  <% end %>
</div>
